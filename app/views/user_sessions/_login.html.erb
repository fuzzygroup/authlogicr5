  <%= form_for @user_session do |f| %>
    <%#= render 'shared/errors', object: @user_session %>
    
    <%=f.hidden_field :login_type, :value => "hyde_web" %>

    <div class="form-group">
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.label :remember_me %>
      <%= f.check_box :remember_me %>
    </div>
    <%= f.submit "Log in!", class: 'btn btn-primary btn-lg ', :style => "text-align: center" %>
    <br/><br/>
    <%#= link_to 'Forgot your password?', new_password_reset_path, class: 'btn btn-sm btn-default' %>
  <% end %>

